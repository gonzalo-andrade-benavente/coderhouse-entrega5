<form action="/productos" method="post" id="my-form">
  <div class="form-group">
    <label for="titleInput">Título del producto</label>
    <input type="text" class="form-control" id="titleInput" name="titleInput" placeholder="Título del producto">
  </div>
   <div class="form-group">
    <label for="priceInput">Precio del producto</label>
    <input type="text" class="form-control" id="priceInput" name="priceInput" placeholder="Precio del producto">
  </div>
  <div class="form-group">
    <label for="thumbnailInput">Thumbnail del producto</label>
    <input type="text" class="form-control" id="thumbnailInput" name="thumbnailInput" placeholder="Thumbnail del producto" value="https://cdn3.iconfinder.com/data/icons/education-209/64/book-note-paper-school-128.png">
    <a href="https://www.iconfinder.com/iconsets/education-209" target="blank"> + url fotos</a>
  </div>
  <button type="submit" class="btn btn-primary">Añadir producto</button>
</form>


